{
    "name": "innmind/http-authentication",
    "type": "library",
    "description": "HTTP authentication",
    "keywords": ["http", "authentication"],
    "homepage": "http://github.com/Innmind/HttpAuthentication",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpAuthentication/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/http": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\HttpAuthentication\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\HttpAuthentication\\": "tests/",
            "Fixture\\Innmind\\HttpAuthentication\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0 || ~8.0",
        "giorgiosironi/eris": "^0.10.0",
        "innmind/compose": "^1.7"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
